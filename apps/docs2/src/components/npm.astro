---
import { PKG_MANGERS, packages } from "@/shared";
import {
  PackageManagers,
  type PackageManagersProps,
} from "starlight-package-managers";

export type Props = PackageManagersProps;
---

<PackageManagers pkgManagers={[...PKG_MANGERS]} {...Astro.props} pkg={Astro.props.pkg && packages(Astro.props.pkg)} />
